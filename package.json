{
  "name": "pytorch-neural-style-gui",
  "productName": "Pytorch Neural Style",
  "version": "0.0.0",
  "description": "An Electron app for the Pytorch implementation of neural style.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "prestart": "node Tools/prestart.js",
    "compile": "pyinstaller neural_style.spec --onedir --noconfirm --workpath=Assets/bin/Builds --distpath=Assets/bin",
    "precompile": "node Tools/precompile.js",
    "prepackage": "node Tools/prepackage.js",
    "package": "electron-packager . pytorch-neural-style-gui --overwrite --asar.unpackDir=\"{Assets/bin,Testing_Assets}\" --ignore=\".gitignore|.vscode|release-builds|requirements.txt|Test.js|Testing_Assets.zip|Assets/Scripts|Assets/bin/Builds\" --platform=win32 --arch=ia32 --prune=true --out release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Pytorch Neural Style\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Razerfish/pytorch-neural-style-gui.git"
  },
  "author": "Alec Wilson",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/Razerfish/pytorch-neural-style-gui/issues"
  },
  "homepage": "https://github.com/Razerfish/pytorch-neural-style-gui#readme",
  "devDependencies": {
    "electron": "^3.0.12",
    "electron-packager": "^13.0.1"
  },
  "dependencies": {}
}
